<table class="add-space-before">
	<thead>
		<tr>
			<th>Exercise</th>
			<th>Time</th>
			<th>Rep</th>
			<th>Weight</th>
			<th>Distance</th>
		</tr>
	</thead>
	<tbody>
		{{#each this.exercises}}
		<tr>
			<td>{{title}}</td>
			<td>{{time}}</td>
			<td>{{rep}}</td>
			<td>{{weight}}</td>
			<td>{{distance}}</td>
		</tr>
		{{/each}}
	</tbody>
</table>